{
  "short_name": "Darumas",
  "name": "Daruma goal tracker",
  "version": "0.0.1",

  "permissons": [
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open Daruma menu"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "icons": {
      "16": "favicon.ico",
      "48": "favicon.ico",
      "128": "logo192.png"
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "manifest_version": 2,

  "content_scripts": [{
    "matches": ["chrome://newtab"],
    "js": [
      "browser-polyfill.js"
    ]
  }],

  "content_security_policy": "script-src 'self' 'sha256-CpqJDQ16JqWmN2jZO3ZN4WczQelekYt28tiobxeP0uQ='; object-src 'self'"
}
